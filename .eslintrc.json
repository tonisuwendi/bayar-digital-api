{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "max-len": ["error", 200]
    }
}
